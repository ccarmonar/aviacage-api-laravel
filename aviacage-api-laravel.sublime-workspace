\tglEnable(GL_LIGHTING);\t\t/* enable lighting */\n\tglEnable(GL_DEPTH_TEST);\t/* enable z buffer */\n\n}\n\n\n\n\nvoid display(void)\n{\n\tglClear(GL_COLOR_BUFFER_BIT | GL_DEPTH_BUFFER_BIT);\n\tglLoadIdentity();\n\tcylinder();\n\tlights();\n\tglutSwapBuffers();\n\tglFlush();\n}\n\nvoid keys(unsigned char key, int x, int y)\n{\t\n\tif(key ==  27) exit(0);\n\n\t//Controles Light 1\n\tif(key == '1') \tglEnable(GL_LIGHT1); //Apagar light0\n\tif(key == '0') \tglDisable(GL_LIGHT1); //Encender light0\n\tif(key == 'x') light2_position[0]-= 1.0;\n\tif(key == 'X') light2_position[0]+= 1.0;\n\tif(key == 'y') light2_position[1]-= 1.0;\n\tif(key == 'Y') light2_position[1]+= 1.0;\n\tif(key == 'z') light2_position[2]-= 1.0;\n\tif(key == 'Z') light2_position[2]+= 1.0;\n\t//cout << \"x=\" << light1_position[0] << \" y=\" << light1_position[1] << \" z=\" << light1_position[2] << endl;\n\n\n\t//Controles Light 2\n\tif(key == '3') \tglEnable(GL_LIGHT0); //Apagar light0\n\tif(key == '2') \tglDisable(GL_LIGHT0); //Encender light0\n\tif(key == 'a') light1_position[0]-= 1.0;\n\tif(key == 'A') light1_position[0]+= 1.0;\n\tif(key == 'b') light1_position[1]-= 1.0;\n\tif(key == 'B') light1_position[1]+= 1.0;\n\tif(key == 'c') light1_position[2]-= 1.0;\n\tif(key == 'C') light1_position[2]+= 1.0;\n\t//cout << \"x=\" << light1_position[0] << \" y=\" << light1_position[1] << \" z=\" << light1_position[2] << endl;\n\n\n\tdisplay();\n}\n\n\nvoid myReshape(int w, int h)\n{\tglViewport(0, 0, w, h);\n\tglMatrixMode(GL_PROJECTION);\n\tglLoadIdentity();\n\tif(w <= h)\n\t\tglOrtho(-4.0,4.0,-4.0*(GLfloat)h/(GLfloat)w,\n      \t\t\t+4.0*(GLfloat)h/(GLfloat)w,-10.0,10.0);\n\telse\n   \t\tglOrtho(-4.0*(GLfloat)w/(GLfloat)h,\n      \t\t\t+4.0*(GLfloat)w/(GLfloat)h,-4.0,4.0,-10.0,10.0);\n\tglMatrixMode(GL_MODELVIEW);\n\tdisplay();\n}\n\n\n\n\n\nvoid myinit(void)\n{ \t\n\tglClearColor(0.0004, 0.4, 0.41, 1.0);\n\tglColor3f(0.2, 0.2, 0.2);\n}\n\n\nvoid showText()\n{\n\tcout<<\" USO DE TECLAS\"<<endl;\n\tcout<<endl;\n\tcout<<\" Fuentes de Luz\"<<endl;\n\tcout<<endl;\n\tcout<<\" 0: Apagar Fuente de Luz 0 (roja)\"<<endl;\n\tcout<<\" 1: Encender Fuente de Luz 0 (roja)\"<<endl;\n\tcout<<\" 2: Apagar Fuente de Luz 1 (verde)\"<<endl;\n\tcout<<\" 3: Encender Fuente de Luz 1 (verde)\"<<endl;\n\tcout<<endl;\n\tcout<<\" Movimientos de las Fuentes de Luz\"<<endl;\n\tcout<<\" x: Mover Luz 0 -x y X: Mover Luz 0 +x\"<<endl;\n\tcout<<\" y: Mover Luz 0 -y y Y: Mover Luz 0 +y\"<<endl;\n\tcout<<\" z: Mover Luz 0 -z y Z: Mover Luz 0 +z\"<<endl;\n\tcout<<endl;\n\tcout<<\" a: Mover Luz 1 -x y A: Mover Luz 1 +x\"<<endl;\n\tcout<<\" b: Mover Luz 1 -y y B: Mover Luz 1 +y\"<<endl;\n\tcout<<\" c: Mover Luz 1 -z y C: Mover Luz 1 +z\"<<endl;\n\tcout<<endl;\n\tcout<<\" ESC = Quit\"<<endl;\n}\n\nint main(int argc, char **argv)\n{\t\n\tshowText();\t\n\tglutInit(&argc, argv);\n\tglutInitDisplayMode(GLUT_DOUBLE | GLUT_RGB | GLUT_DEPTH);\n\tglutInitWindowSize(500, 500);\n\tglutCreateWindow(\"GRUPO 03 - T4\");\n\tmyinit();\n\tglutReshapeFunc(myReshape);\n\tglutDisplayFunc(display);\n\tglutKeyboardFunc(keys);\n\tglutMainLoop();\n\treturn 0;\n}\n",
